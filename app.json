{
  "expo": {
    "name": "Haikal App",
    "slug": "haikal-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-tpq.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-tpq.png",
      "resizeMode": "contain",
      "backgroundColor": "#F50057"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-tpq.png",
        "backgroundColor": "#F50057"
      },
      "package": "com.haikal.haikalapp"
    },
    "web": {
      "favicon": "./assets/icon-tpq.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "firebase-auth-template",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "90b402a4-d9ce-4b0f-9dfe-ffae8d4a0f40"
      },
      "midtrans": {
        "environment": "sandbox",
        "clientKeySandbox": "SB-Mid-client-SANDBOX_KEY_HERE",
        "clientKeyProduction": "Mid-client-PRODUCTION_KEY_HERE"
      }
    }
  }
}
